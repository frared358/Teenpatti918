                <TextView
                    android:id="@+id/user_id4"
                    android:layout_alignParentEnd="true"
                    app:layout_heightPercent="20%"
                    app:layout_widthPercent="20%"
                    android:layout_alignParentRight="true"/>

            </android.support.percent.PercentRelativeLayout>

