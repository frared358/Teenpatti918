    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/privatetble"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.percent.PercentRelativeLayout
        android:id="@+id/privaterecycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg3">

        <android.support.percent.PercentRelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <!--Left side ithree_two_one_leaderboardyout-->
            <android.support.percent.PercentRelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/back"
                    android:src="@drawable/back"
                    app:layout_heightPercent="10%"
                    app:layout_marginLeftPercent="1%"
                    app:layout_marginTopPercent="1%"
                    app:layout_widthPercent="25%" />


            </android.support.percent.PercentRelativeLayout>

            <!--Right side ithree_two_one_leaderboardyout-->
            <android.support.percent.PercentRelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true">


                <ImageView
                    android:id="@+id/imgVInfo"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/infogame"
                    app:layout_heightPercent="10%"
                    app:layout_marginLeftPercent="1%"
                    app:layout_marginTopPercent="1%"
                    app:layout_widthPercent="8%" />

            </android.support.percent.PercentRelativeLayout>
        </android.support.percent.PercentRelativeLayout>

        <android.support.percent.PercentRelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true">


            <ImageView
                android:id="@+id/table"
                android:layout_width="410dp"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:src="@drawable/teenpattitableimage"
                app:layout_widthPercent="70%" />

            <android.support.percent.PercentRelativeLayout
                android:layout_centerInParent="true"
                app:layout_heightPercent="25%"
                app:layout_widthPercent="30%">

                <!--<com.akexorcist.roundcornerprogressbar.RoundCornerProgressBar-->
                    <!--android:id="@+id/progressChaalTimer"-->
                    <!--android:layout_centerHorizontal="true"-->
                    <!--android:layout_above="@+id/txtVTableAmt"-->
                    <!--app:rcRadius="5dp"-->
                    <!--app:rcBackgroundPadding="2dp"-->
                    <!--app:rcReverse="true"-->
                    <!--app:rcProgressColor="@color/Black"-->
                    <!--app:rcBackgroundColor="@color/colorYellow"-->
                    <!--app:layout_heightPercent="8%"-->
                    <!--app:layout_widthPercent="70%"/>-->


                <TextView
                    android:id="@+id/txtVTableAmt"
                    android:layout_height="37dp"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:text="5000"
                    android:textColor="@color/white"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_heightPercent="45%"
                    app:layout_widthPercent="80%" />
            </android.support.percent.PercentRelativeLayout>



            <android.support.percent.PercentRelativeLayout
                android:id="@+id/rl_playerbg2"
                android:layout_alignTop="@+id/table"
                app:layout_heightPercent="30%"
                app:layout_marginTopPercent="12%"
