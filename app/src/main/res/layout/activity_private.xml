                    android:src="@drawable/inner_img_profile"
                    app:layout_heightPercent="88%"
                    app:layout_widthPercent="88%" />

                <TextView
                    android:id="@+id/nametext2"
                    android:layout_alignTop="@id/inner_player_img2"
                    android:background="@color/colorYellowGolden"
                    android:textAlignment="center"
                    android:textColor="@color/Black"
                    app:layout_heightPercent="20%"
