                    android:layout_alignParentEnd="true"
                    app:layout_heightPercent="20%"
                    app:layout_widthPercent="20%"
                    android:layout_alignParentRight="true"/>

            </android.support.percent.PercentRelativeLayout>

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/rl_playerbg3"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/table"
                app:layout_heightPercent="30%"
                app:layout_marginTopPercent="12%"
                app:layout_widthPercent="20%">

                <ImageView
                    android:id="@+id/player_blink_circle3"
                    android:src="@drawable/yellow_circle"
                    app:layout_heightPercent="100%"
                    app:layout_widthPercent="100%" />

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/inner_player_img3"
                    android:layout_centerInParent="true"
                    android:src="@drawable/inner_img_profile"
