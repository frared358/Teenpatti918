        <android.support.percent.PercentRelativeLayout
            android:id="@+id/rl_bottom_caption"
            android:layout_alignParentBottom="true"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="gone"
            app:layout_heightPercent="8%"
            app:layout_widthPercent="100%">

            <TextView
                android:id="@+id/pack_btn"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/action_btn"
                android:text="PACK"
                android:textAlignment="center"
                android:textColor="@color/darkbrown"
                android:textSize="18sp"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="10%"
                app:layout_widthPercent="12%" />

            <ImageView
                android:id="@+id/minus_btn"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@+id/pack_btn"
                android:src="@drawable/minus_disabled"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_widthPercent="8%" />

            <ImageView
                android:id="@+id/amount_bg"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@+id/minus_btn"
                android:src="@drawable/emptybtn"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_widthPercent="22%" />

            <TextView
                android:id="@+id/start_amount"
                android:layout_alignParentBottom="true"
                android:layout_alignTop="@+id/amount_bg"
                android:layout_centerInParent="true"
                android:layout_toRightOf="@+id/minus_btn"
                android:padding="1dp"
                android:text="2"
                android:background="@drawable/empty_btn"
                android:textAlignment="center"
                android:textColor="@color/white"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_widthPercent="22%" />


            <ImageView
                android:id="@+id/plus_btn"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@+id/start_amount"
                android:src="@drawable/plus_btn"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_widthPercent="8%" />

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/rlChaalBlind"
                android:layout_toRightOf="@+id/plus_btn"
                android:layout_alignParentBottom="true"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_widthPercent="12%">

                <TextView
                    android:id="@+id/chaal_btn"
                    android:background="@drawable/action_btn"
                    android:text="CHAAL"
                    android:textAlignment="center"
                    android:textColor="@color/darkbrown"
                    android:textSize="18sp"
                    android:visibility="gone"
                    app:layout_heightPercent="100%"
                    app:layout_widthPercent="100%"/>


                <TextView
                    android:id="@+id/blind_btn"
                    android:background="@drawable/action_btn"
                    android:text="BLIND"
                    android:textAlignment="center"
                    android:textColor="@color/darkbrown"
                    android:textSize="18sp"
                    app:layout_heightPercent="100%"
                    app:layout_widthPercent="100%"/>

            </android.support.percent.PercentRelativeLayout>



            <TextView
                android:id="@+id/show_btn"
                android:layout_alignParentBottom="true"
                android:layout_toRightOf="@+id/rlChaalBlind"
                android:background="@drawable/action_btn"
                android:text="SHOW"
                android:textAlignment="center"
                android:textColor="@color/darkbrown"
                android:textSize="18sp"
                android:visibility="invisible"
                app:layout_heightPercent="100%"
                app:layout_marginLeftPercent="1%"
                app:layout_marginRightPercent="5%"
                app:layout_widthPercent="12%" />
        </android.support.percent.PercentRelativeLayout>

        <LinearLayout
            android:id="@+id/below_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:background="#e9250400"
            android:gravity="center"
            android:padding="5dp"
            android:visibility="visible">

            <TextView
                android:id="@+id/boot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/table_boot"
                android:textColor="@color/colorYellowDark" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorYellowDark" />

            <TextView
                android:id="@+id/pot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/table_pot"
                android:textColor="@color/colorYellowDark" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorYellowDark" />

            <TextView
                android:id="@+id/max"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/table_max"
                android:textColor="@color/colorYellowDark" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorYellowDark" />
        </LinearLayout>

    </android.support.percent.PercentRelativeLayout>

</RelativeLayout>




