                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/rl_myplayer"
                android:layout_toRightOf="@+id/rl_myplayer"
                android:background="@drawable/see_btn"
                android:padding="5dp"
                android:text="SEE"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:visibility="gone"
                app:layout_marginTopPercent="10%" />

            <TextView
                android:id="@+id/display_myplayer_bind"
                android:layout_alignParentLeft="true"
                android:text="PACKED"
                android:textAlignment="center"
                android:textColor="@color/white"
                app:layout_heightPercent="10%"
                app:layout_marginLeftPercent="42%"
                app:layout_marginTopPercent="70%"
                app:layout_widthPercent="15%"
                tools:ignore="NotSibling" />

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/rl_myplayer"
                android:layout_alignTop="@+id/table"
                android:layout_centerHorizontal="true"
                app:layout_heightPercent="30%"
                app:layout_marginTopPercent="60%"
                app:layout_widthPercent="20%"
                tools:ignore="NotSibling">



                <ImageView
                    android:id="@+id/myplayer"
                    android:src="@drawable/yellow_circle"
                    app:layout_heightPercent="100%"
                    app:layout_widthPercent="100%"
                    android:visibility="visible"/>

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/inner_player_img"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/inner_img_profile"
                    app:layout_heightPercent="88%"
                    app:layout_widthPercent="88%" />

                <com.dinuscxj.progressbar.CircleProgressBar
                    android:id="@+id/progressBarChances"
                    app:style="solid"
                    android:layout_marginStart="7.5dp"
                    android:layout_marginTop="6.5dp"
                    app:progress_shader="radial"
                    app:progress_stroke_width="1dp"
                    app:progress_background_color="#0162ff00"
                    app:progress_start_color="#9662ff00"
                    app:layout_heightPercent="89%"
                    app:layout_widthPercent="89%"  />


                <ImageView
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:src="@drawable/stars"
                    android:id="@+id/winnerblinker1"
                    android:visibility="invisible"/>

                <!--<ImageView-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:src="@drawable/stars2"-->
                    <!--android:id="@+id/winnerblinker2"-->
                    <!--android:visibility="invisible"/>-->


                <TextView
                    android:id="@+id/nametext"
                    android:layout_alignTop="@id/inner_player_img"
                    android:background="@color/colorYellowGolden"
                    android:textAlignment="center"
                    android:textColor="@color/Black"
                    app:layout_heightPercent="20%"
